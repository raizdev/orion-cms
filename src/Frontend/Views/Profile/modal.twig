<div id="modal-store___BV_modal_outer_" class="modal-inventory" style="position: absolute; z-index: 1040;display: none">
   <div id="modal-store" role="dialog" aria-describedby="modal-store___BV_modal_body_" class="modal fade show" aria-modal="true" style="display: block; padding-right: 17px;">
      <div class="modal-dialog modal-lg modal-dialog-centered">
         <span tabindex="0"></span>
         <div id="modal-store___BV_modal_content_" tabindex="-1" class="modal-content">
            <div id="modal-store___BV_modal_body_" class="modal-body">
               <div class="btn btn-dark float-right close-inventory"><span aria-hidden="true">Ã—</span></div>
               <button class="btn btn-secondary btn-success">
               <span>Store</span> 
               </button> 
               <div class="row mx-n1 pt-3">
                  <div class="col-1">
                     <div class="store-list-overflow myInventory">
                        <div class="btn btn-dark btn-sm w-100 sticker-cat-open" data-type="s">
                           Stickers
                        </div>
                         <div id="stickers-cat" class="mt-2 collapse" style="display: none;"> 
                            <div class="list-group"></div>
                         </div>
                        <div id="background-cat" class="btn btn-dark btn-sm w-100 mt-2" data-type="b">
                           Backgrounds
                        </div>
                        <div id="widgets-cat"class="btn btn-dark btn-sm w-100 mt-2" data-type="w">
                           Widgets
                        </div>
                     </div>
                  </div>
                  <div class="stickers col-2 stickers-data" style="display: none">
                     <div class="store-list-overflow">
                        <div class="store-item-grid" data-type="s"></div>
                     </div>
                  </div>
                  <div class="widgets col-2 widgets-data" style="display: none">
                     <div class="store-list-overflow">
                        <div class="list-group store-widget-list-grid">
                           <div>
                              {% for widget in data.player.widgets %}
                                 {% if widget.cat.data != "note" and widget.type == "w" and widget.cat.data != "myhabbo" %}
                                 <div class="list-group-item store-widget-list-grid-item p-1">
                                    <div class="d-flex flex-row w-100">
                                       <div class="store-widget-list-grid-item-preview flex-shrink-0 {{widget.cat.data}} mr-2"></div>
                                       <div class="w-100"><span class="d-block">{{ widget.cat.name }}</span></div>
                                        <button class="llbtn llbtn-sm align-self-center llbtn-{% if widget.hide %}success{% else %}danger{% endif %} widgetButton" data-status="{% if widget.hide %}0{% else %}1{% endif %}" data-id="{{widget.id}}">{% if widget.hide %}Show{% else %}Hide{% endif %}</button>
                                    </div>
                                 </div>
                                 {% endif %}
                              {% endfor %}
                                 <div class="list-group-item store-widget-list-grid-item p-1">
                                    <div class="d-flex flex-row w-100">
                                       <div class="store-widget-list-grid-item-preview flex-shrink-0 note mr-2"></div>
                                       <div class="w-100"><span class="d-block">Note</span></div>
                                        <button class="llbtn llbtn-sm align-self-center llbtn-success addNote">Add</button>
                                    </div>
                                 </div>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="col-1">
                     <div class="store-list-overflow overview" style="display: none">
                          <button class="llbtn llbtn-success llbtn-sm w-100 mb-2 placeButton">
                           Place
                           </button>  
                           <img class="align-self-center flex-shrink-0 flex-grow-0 overview-preview">
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <span tabindex="0"></span>
      </div>
   </div>
   <div id="modal-store___BV_modal_backdrop_" class="modal-backdrop"></div>
</div>